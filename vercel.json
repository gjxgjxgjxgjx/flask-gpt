{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/main.py"
    }
  ],
  "functions": {
    "api/test.py": {
      "memory": 3008,
      "maxDuration": 30
    },
    "api/*.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
